# Server
PORT=3000
NODE_ENV=development

# MongoDB
# Example for local MongoDB
MONGO_URI=mongodb://localhost:27017/yaacht

# Nausys API credentials
# Obtain from Nausys; required for API sync
NAUSYS_USERNAME=
NAUSYS_PASSWORD=

# Optional: crew list security code (enables crew sync)
NAUSYS_CREW_SECURITY_CODE=

# Optional: logging level (info | debug | warn | error)
LOG_LEVEL=info
